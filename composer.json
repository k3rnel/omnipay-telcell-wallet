{
  "name": "k3rnel/omnipay-telcell-wallet",
  "version": "1.0",
  "type": "library",
  "description": "TelcellWallet gateway for Omnipay payment processing library",
  "keywords": [
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "telcell",
    "telcell-wallet",
    "purchase"
  ],
  "homepage": "https://github.com/k3rnel/omnipay-telcell-wallet",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Omnipay\\TelcellWallet\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Poghos Boyajyan",
      "email": "poghosboyajyan@gmail.com"
    }
  ],
  "require": {
    "omnipay/common": "^3.0",
    "ext-json": "*",
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.8",
    "php-http/guzzle7-adapter": "^1.0"
  },
  "require-dev": {
    "omnipay/tests": "^4",
    "http-interop/http-factory-guzzle": "^1.2",
    "symfony/var-dumper": "^6.4",
    "friendsofphp/php-cs-fixer": "^3.40"
  },
  "scripts": {
    "style": [
      "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    ],
    "test": [
      "vendor/bin/phpunit --testsuite \"Unit\""
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
